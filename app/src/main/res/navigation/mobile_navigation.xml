<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.android.gfhl.fragments.FragmentChampions"
        android:label="@string/champions"
        tools:layout="@layout/champion_list" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.android.gfhl.fragments.FragmentItems"
        android:label="@string/items"
        tools:layout="@layout/item_list" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.android.gfhl.fragments.FragmentFavs"
        android:label="@string/favorites"
        tools:layout="@layout/champion_list" />
</navigation>